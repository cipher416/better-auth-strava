{
  "name": "@better-auth/strava",
  "version": "0.1.0",
  "description": "Strava OAuth provider for better-auth",
  "type": "module",
  "private": false,
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "check-types": "tsc --noEmit -p tsconfig.json",
    "prepublishOnly": "bun run clean && bun run build"
  },
  "peerDependencies": {
    "better-auth": "^1.3.0"
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "typescript": "5.9.2"
  },
  "keywords": [
    "better-auth",
    "strava",
    "oauth2",
    "authentication"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
